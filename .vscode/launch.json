{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Next.js Debug",
			"program": "${workspaceFolder}/node_modules/next/dist/bin/next",
			"args": [
				"dev"
			],
			"cwd": "${workspaceFolder}",
			"protocol": "inspector",
			"runtimeArgs": [
				"--inspect"
			],
			"console": "integratedTerminal",
			"outputCapture": "std"
		}
	]
}